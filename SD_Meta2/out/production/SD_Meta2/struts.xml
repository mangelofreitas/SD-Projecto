<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.devMode" value="true"/>

    <package name="default" extends="struts-default">
        <action name="profile">
            <result>/profile.jsp</result>
        </action>
        <action name="login" class="action.Login">
            <result name="success">/profile.jsp</result>
            <result name="index">/index.jsp</result>
        </action>
        <action name="regist" class="action.Regist">
            <result name="success">/profile.jsp</result>
            <result name="index">/index.jsp</result>
        </action>
        <action name="create" class="action.CreateProject">
            <result name="success">/profile.jsp</result>
            <result name="index">/newproject.jsp</result>
        </action>
        <action name="newproject" class="action.MenuHandling" method="execute">
            <result name="success">/newproject.jsp</result>
        </action>
        <action name="myprojects" class="action.ListMyProjects">
            <result name="success">/myprojects.jsp</result>
        </action>
    </package>
</struts>